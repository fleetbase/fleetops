<Layout::Resource::Panel
    @resource={{this.serviceRate}}
    @controller={{this}}
    @actionButtons={{this.actionButtons}}
    @headerTitle={{t "common.create-a-new-resource" resource=(lowercase (t "resource.service-rate"))}}
    @saveTask={{this.save}}
    @onPressCancel={{transition-to "operations.service-rates.index"}}
    @onOverlayReady={{fn (mut this.overlay)}}
>
    <ServiceRate::Form @resource={{this.serviceRate}} @controller={{this}} />
    <Spacer @height="200px" />
</Layout::Resource::Panel>