<Layout::Resource::Panel
    @resource={{@model}}
    @controller={{this}}
    @headerTitle={{concat "Edit report: " @model.title}}
    @actionButtons={{this.actionButtons}}
    @saveTask={{this.save}}
    @onPressCancel={{this.cancel}}
    @onOverlayReady={{fn (mut this.overlay)}}
    @width="800px"
>
    <ReportBuilder @report={{@model}} />
    <Spacer @height="300px" />
</Layout::Resource::Panel>
