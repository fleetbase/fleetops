<Layout::Resource::Panel
    @resource={{this.fleet}}
    @controller={{this}}
    @actionButtons={{this.actionButtons}}
    @headerTitle="Create a new fleet"
    @saveTask={{this.save}}
    @onPressCancel={{transition-to "management.fleets.index"}}
    @onOverlayReady={{fn (mut this.overlay)}}
>
    <Fleet::Form @resource={{this.fleet}} @controller={{this}} />
    <Spacer @height="200px" />
</Layout::Resource::Panel>

