<Layout::Resource::Panel
    @resource={{this.vehicle}}
    @controller={{this}}
    @actionButtons={{this.actionButtons}}
    @headerTitle="Create a new vehicle"
    @saveTask={{this.save}}
    @onPressCancel={{transition-to "management.vehicles.index"}}
    @onOverlayReady={{fn (mut this.overlay)}}
>
    <Vehicle::Form @resource={{this.vehicle}} @controller={{this}} />
    <Spacer @height="200px" />
</Layout::Resource::Panel>

