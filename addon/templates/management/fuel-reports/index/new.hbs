<Layout::Resource::Panel
    @resource={{this.fuelReport}}
    @controller={{this}}
    @actionButtons={{this.actionButtons}}
    @headerTitle="Create a new fuel report"
    @saveTask={{this.save}}
    @onPressCancel={{transition-to "management.fuel-reports.index"}}
    @onOverlayReady={{fn (mut this.overlay)}}
>
    <FuelReport::Form @resource={{this.fuelReport}} @controller={{this}} class="px-4" />
</Layout::Resource::Panel>

