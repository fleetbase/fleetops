<Layout::Resource::Panel
    @resource={{@model}}
    @controller={{this}}
    @headerTitle={{concat "Edit: fuel reported on " @model.createdAt}}
    @actionButtons={{this.actionButtons}}
    @saveTask={{this.save}}
    @onPressCancel={{this.cancel}}
    @onOverlayReady={{fn (mut this.overlay)}}
>
    <FuelReport::Form @resource={{@model}} @controller={{this}} class="px-4" />
</Layout::Resource::Panel>

