<Layout::Resource::Panel
    @resource={{this.contact}}
    @controller={{this}}
    @actionButtons={{this.actionButtons}}
    @headerTitle="Create a new contact"
    @saveTask={{this.save}}
    @onPressCancel={{transition-to "management.contacts.index"}}
    @onOverlayReady={{fn (mut this.overlay)}}
>
    <Contact::Form @resource={{this.contact}} @controller={{this}} />
    <Spacer @height="200px" />
</Layout::Resource::Panel>

